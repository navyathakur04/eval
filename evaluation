students = {
    "Navya": {"class": "10" , "height" : 175 , "weight" : 80, "sport" : "tennis", "calorie": 2100}
    "smriti": {"class": "11" , "height" : 178 , "weight" : 75, "sport" : "cricket", "calorie": 2400}
